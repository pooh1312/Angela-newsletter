<%-include("header")-%>
        <div class="box"  id="heading">
            <h1 style="color:indigo"><%= listname %> </h1>
        </div>

        <div class="box">
            <% for(var i=0; i<newitems.length;i++){%>
            <div class="item">
                <input type="checkbox">
                <p><%= newitems[i] %></p>
            </div>
            <% }%>

            <form class="item" action="/" method="post" >
                <input type="text" name="itemName" placeholder="add item" autocomplete="off">
                <button type="submit" name="p" value=<%= listname%>>+</button>
            </form>
        </div>
        
        <script src="" async defer></script>

        <%-include("footer")-%>
   